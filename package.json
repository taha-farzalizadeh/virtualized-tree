{
  "name": "virtualized-tree-react",
  "version": "1.0.0",
  "description": "A customizable virtualized tree component with sorting and expandable nodes, built with React and TypeScript.",
  "main": "dist/virtualized-tree.umd.js",
  "module": "dist/virtualized-tree.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/virtualized-tree.es.js",
      "require": "./dist/virtualized-tree.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,css}'",
    "tsc": "tsc"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "lucide-react": "^0.283.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.7"
  },
  "devDependencies": {
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.1",
    "jest": "^29.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.7.2",
    "vite": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taha-farzalizadeh/virtualized-tree.git"
  },
  "bugs": {
    "url": "https://github.com/taha-farzalizadeh/virtualized-tree/issues"
  },
  "homepage": "https://github.com/taha-farzalizadeh/virtualized-tree#readme",
  "keywords": [
    "react",
    "tree",
    "virtualized-tree",
    "react-window-tree",
    "expandable-tree",
    "sortable-tree",
    "react-tree",
    "virtualization",
    "customizable-tree"
  ],
  "author": "tahaff79@gmail.com",
  "license": "MIT",
  "sideEffects": false
}
